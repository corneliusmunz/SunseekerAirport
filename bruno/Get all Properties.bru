meta {
  name: Get all Properties
  type: http
  seq: 13
}

post {
  url: {{baseUrl}}/api/iot_mower/wireless/device/get_property
  body: json
  auth: inherit
}

headers {
  Accept-Language: de
  Content-Type: application/json
  ~Authorization: Bearer {{access_token}}
}

body:json {
  {
    "appId": {{user_id}},
    "deviceSn": "{{deviceSerialNumber}}",
    "id": "getDevAllProperty",
    "key": "all",
    "method": "get_property"
  }
}
